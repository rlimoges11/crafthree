{% macro print(scene) %}
    {
    {% for block in scene %}
        {% if block.type == "sphere" %}
            "sphere{{ block.id }}":{
            "type": "{{ block.type }}",
            "radius": "{{ block.radius }}",
            "color": "{{ block.color }}"
            },

        {% endif %}
    {% endfor %}
    }
{% endmacro %}