{% macro printArray(scene) %}
    <script>
        var json = [];
        {% for block in scene %}
        var data = [];

        {% if block.type == "planet" %}
        data["name"] = "{{ block.type }}-{{ block.id }}";
        data["type"] = "{{ block.type }}";
        data["id"] = "{{ block.id }}";
        data["color"] = "{{ block.color.hex }}";
        data["radius"] = {{ block.radius }};
        data["x"] = {{ block.posX }};
        data["z"] = {{ block.posZ }};
        {% endif %}


        json.push(data);

        {% endfor %}
    </script>
{% endmacro %}